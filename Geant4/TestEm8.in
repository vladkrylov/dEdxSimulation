#
# Macro file for the initialization phase of "TestEm8.cc"
#
# It creates the default geometry (simple absorber cylinder )        
#
/control/verbose 2
/run/verbose 2
#/tracking/verbose 2
#
#  select EM PhysicsList
#
/testem/phys/addPhysics emstandard_opt1
/testem/phys/addPhysics pai

/cuts/setLowEdge 200.00 eV
/cuts/setHighEdge 100.00 GeV

#/testem/phys/addPhysics  pai_photon
#
#/testem/stepMax         10 mm
#
/testem/phys/setNbinsE 20
/testem/phys/setMaxE 0.060  MeV

# /testem/histo/RootTreeName test1234
#
/run/initialize
#
/run/setCut  0.1 mm
/run/setCutForRegion GasDetector 0.1 mm
/run/particle/dumpCutValues
#
/gun/particle e-
#
#/gun/particle proton
#
#/gun/particle mu-
# 
/gun/energy 3.00 MeV


/run/beamOn 1500

